import random
counter = 1
password = ""

alphabet = "abcdefghijklmnopqrstuvwxyzABCDEFGHIJKLMNOPQRSTUVWXYZ1234567890"

num_of_characters = random.randint(8, 16)

while counter < num_of_characters:
    character_num = random.randint(1, 62)
    character = alphabet[character_num]
    password = password + character
    counter += 1

print("Password :", password)
